<%- include('partials/header'); -%>

<% posts.forEach((post) => { %>
    <div class="container posts" id="posts">
        <p class="full-name"><i class="fa-solid fa-user"></i><%= post.fullName %></p>
        <h2 class="post-title"><%= post.title %></h2>
        <p class="content-post">
            <%= post.content %>
        </p>
        <span id="readMore"></span>
    </div>
<% }) %>

<%- include('partials/footer'); -%>