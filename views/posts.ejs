<%- include('partials/header'); -%>

<% posts.forEach((post) => { %>
    <div class="container profile-posts">
        <h2 class="post-title"><%= post.title %></h2>
        <p class="content-post">
            <%= post.content.substring(0, 100) + " ..." %>
            <a href="">Read more</a>
        </p>
    </div>
<% }) %>

<%- include('partials/footer'); -%>